# --------------------------------------------------------------------------------------------
#Name Qtd Meses
#Desc Define quantidade de meses parametrizados para a cópia
#Date 27/08/2020
# --------------------------------------------------------------------------------------------
['Qtd Meses'] =S: 
STR( IF(  DB( 'SYS.900.Parametros_Copia_Versao', !SYS.D.900.Valor, 'Período Início' ) @= ''
   % DB( 'SYS.900.Parametros_Copia_Versao', !SYS.D.900.Valor, 'Período Fim' ) @= ''
	, 0
    , CONTINUE
)
, 2, 0 );


['Qtd Meses'] =S: 
STR( DIMIX( 'SYS.D.Copia_Versao_Ano_Mes', DB( 'SYS.900.Parametros_Copia_Versao', !SYS.D.900.Valor, 'Período Fim' ))
   - DIMIX( 'SYS.D.Copia_Versao_Ano_Mes', DB( 'SYS.900.Parametros_Copia_Versao', !SYS.D.900.Valor, 'Período Início' )) + 1
, 2, 0 );